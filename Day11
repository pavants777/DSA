https://leetcode.com/problems/search-insert-position/description/



class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        for(int i=0;i<nums.size();i++){
            if(target==nums[i])
             return i;
            else if(nums[i]>target){
                return i;
            }
        }
        return nums.size();
    }
};
