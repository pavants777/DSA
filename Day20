https://leetcode.com/problems/valid-anagram/
https://leetcode.com/problems/reverse-string/


class Solution {
public:
    bool isAnagram(string s, string t) {
        sort(s.begin(),s.end());
        sort(t.begin(),t.end());
        if(s == t) return true;
        return false;
    }
};

class Solution {
public:
    void reverseString(vector<char>& s) {
        reverse(s.begin(),s.end());
    }
};
