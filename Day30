https://leetcode.com/problems/power-of-two/?envType=daily-question&envId=2024-02-19


class Solution {
public:
    bool isPowerOfTwo(int n) {
         int i = 0;

         while(i<n){
             if(pow(2,i)==n) return true;
             if(pow(2,i)>n) return false;
             i++;
         }
         return false;
    }
};
